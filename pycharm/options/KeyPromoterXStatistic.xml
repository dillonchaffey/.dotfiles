<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Add Bookmark">
        <Item>
          <IdeaActionID>ToggleBookmark</IdeaActionID>
          <ShortCut>'F11'</ShortCut>
          <Description>Add Bookmark</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Bookmarks">
        <Item>
          <IdeaActionID>ActivateBookmarksToolWindow</IdeaActionID>
          <ShortCut>'Alt+2'</ShortCut>
          <Description>Bookmarks</Description>
          <Count>10</Count>
          <Hit>3</Hit>
        </Item>
      </Statistic>
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'Ctrl+C' or 'Ctrl+Insert'</ShortCut>
          <Description>Copy</Description>
          <Count>7</Count>
          <Hit>33</Hit>
        </Item>
      </Statistic>
      <Statistic Action="Delete…">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'Delete'</ShortCut>
          <Description>Delete…</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Generate...">
        <Item>
          <IdeaActionID>Generate</IdeaActionID>
          <ShortCut>'Alt+Insert'</ShortCut>
          <Description>Generate...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Go to Declaration or Usages">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B'</ShortCut>
          <Description>Go to Declaration or Usages</Description>
          <Count>7</Count>
        </Item>
      </Statistic>
      <Statistic Action="Move Directory…">
        <Item>
          <IdeaActionID>Move</IdeaActionID>
          <ShortCut>'F6'</ShortCut>
          <Description>Move Directory…</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Move File…">
        <Item>
          <IdeaActionID>Move</IdeaActionID>
          <ShortCut>'F6'</ShortCut>
          <Description>Move File…</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Paste">
        <Item>
          <IdeaActionID>$Paste</IdeaActionID>
          <ShortCut>'Ctrl+V' or 'Shift+Insert'</ShortCut>
          <Description>Paste</Description>
          <Count>1</Count>
          <Hit>16</Hit>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'Alt+1'</ShortCut>
          <Description>Project</Description>
          <Count>15</Count>
          <Hit>8</Hit>
        </Item>
      </Statistic>
      <Statistic Action="Rename…">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'Shift+F6'</ShortCut>
          <Description>Rename…</Description>
          <Count>2</Count>
          <Hit>3</Hit>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Context Actions">
        <Item>
          <IdeaActionID>ShowIntentionActions</IdeaActionID>
          <ShortCut>'Alt+Enter'</ShortCut>
          <Description>Show Context Actions</Description>
          <Count>1</Count>
          <Hit>7</Hit>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Alt+F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>